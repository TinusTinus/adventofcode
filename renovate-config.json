{
  "onboarding": false,
  "requireConfig": "optional",
  "branchPrefix": "renovate-action/",
  "repositories": [
    "TinusTinus/adventofcode"
  ],
  "packageRules": [
    {
      "description": "Automerge non-major updates",
      "matchUpdateTypes": ["minor", "patch"],
      "automerge": true
    },
    {
      "description": "Prevent automatic Kotlin updates",
      "matchPackageNames": ["org.jetbrains.kotlin:*"],
      "enabled": false
    }
  ]
}